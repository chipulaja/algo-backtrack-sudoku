{
    "name": "chipulaja/algo-backtrack-sudoku",
    "description": "a library for solving sudoku",
    "keywords": ["backtrack","sudoku"],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Chipulaja\\Algo\\Backtrack\\Sudoku\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "scrutinizer/ocular": "^1.8"
    },
    "authors": [
        {
            "name": "chipulaja",
            "email": "chipul.aja@gmail.com"
        }
    ],
    "scripts": {
        "cs-check": "phpcs --standard=PSR2 -n src",
        "cs-fix": "phpcbf --standard=PSR2 -n src",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html build"
    },
    "license": "MIT"
}
